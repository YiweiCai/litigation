	         <result name="list">/WEB-INF/jsp/system/user/listUser.jsp</result>
	         <result name="add">/WEB-INF/jsp/system/user/addUser.jsp</result>
	         <result name="tree">/WEB-INF/jsp/system/user/userTree.jsp</result>

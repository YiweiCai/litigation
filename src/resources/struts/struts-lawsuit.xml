	   		<result name="weiTiJiao">/WEB-INF/jsp/lawsuit/wsla/weiTiJiao.jsp</result>
	    </action>
	    
	     <action name="xf_*" class="XfAction" method="{1}">
	         <result name="add">/WEB-INF/jsp/lawsuit/wsxf/addXf.jsp</result>
	         <result name="list">/WEB-INF/jsp/lawsuit/wsxf/listXf.jsp</result>
	         <result name="flist">/WEB-INF/jsp/lawsuit/wsxf/flistXf.jsp</result>
	         <result name="replyXf">/WEB-INF/jsp/lawsuit/wsxf/replyXf.jsp</result>
	         <result name="detail">/WEB-INF/jsp/lawsuit/wsxf/detailXf.jsp</result>
	         <result name="fdetail">/WEB-INF/jsp/lawsuit/wsxf/fdetailXf.jsp</result>
	    </action>
	    <action name="lo_*" class="LawyerOnlineAction" method="{1}">
	         <result name="add">/WEB-INF/jsp/lawsuit/lo/contactlayer.jsp</result>
	         <result name="list">/WEB-INF/jsp/lawsuit/lo/list.jsp</result>
	         <result name="flist">/WEB-INF/jsp/lawsuit/lo/contactlayerlist.jsp</result>
	         <result name="edit">/WEB-INF/jsp/lawsuit/lo/edit.jsp</result>
	         <result name="view">/WEB-INF/jsp/lawsuit/lo/view.jsp</result>
	    </action>
	    <action name="co_*" class="CourtOpeningAction" method="{1}">
	         <result name="list">/WEB-INF/jsp/lawsuit/co/list.jsp</result>
	         <result name="flist">/WEB-INF/jsp/lawsuit/co/flist.jsp</result>
	         <result name="edit">/WEB-INF/jsp/lawsuit/co/edit.jsp</result>
	         <result name="view">/WEB-INF/jsp/lawsuit/co/view.jsp</result>
	         <result name="uploadVideo">/WEB-INF/jsp/lawsuit/co/uploadVideo.jsp</result>
	    </action>
	    <action name="uv_*" class="UploadVideoAction" method="{1}">
	    	<result name="list">/WEB-INF/jsp/lawsuit/co/listVideo.jsp</result>
	    	<result name="flist">/WEB-INF/jsp/lawsuit/co/flistVideo.jsp</result>
	    	<result name="edit">/WEB-INF/jsp/lawsuit/co/editVideo.jsp</result>
	    	<result name="view">/WEB-INF/jsp/lawsuit/co/viewVideo.jsp</result>
			<result name="sufeijisuan">/WEB-INF/jsp/lawsuit/sfjs/suFeiJiSuan.jsp</result>
	    </action>
	    <!-- Cai -->
	    <!-- S 开庭公告信息 -->
	    	<!-- 添加开庭公告 -->
	    	<action name="courtOpenInfo" class="com.lawsuit.wsla.action.CourtOpenInfoAction" method="executeMethod">
	    	 	<result name="success">/WEB-INF/jsp/index.jsp</result>
	    	</action>
